
  {
    "version": 2,
    "builds": [
      { "src": "app.py", "use": "@vercel/python" },
      { "src": "index.html", "use": "@vercel/static" },
      { "src": "model.js", "use":"@vercel/javascript"}
    ],
    "routes": [
      { "src": "/.*", "dest": "/app.py" },
      { "src": "/", "dest": "/index.html" },
      { "src": "/api/*", "dest": "/api" }
    ]
  }

  {
    "version": 2,
    "builds": [
      { "src": "app.py", "use": "@vercel/python" },
      { "src": "index.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/.*", "dest": "/app.py" },
      { "src": "/", "dest": "/index.html" }
    ],
    "functions": {
      "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
    }
  }
{
    "name": "traffic-labb",
    "version": "1.0.0",
    "description": "Traffic Simulation Web Application",
    "main": "index.js",
    "scripts": {
        "predev": "bash ensure-env.sh",
        "dev": "node dev-server.js",
        "cleanup": "bash cleanup-ports.sh",
        "cleanup:project": "bash cleanup-project.sh",
        "check": "bash pre-deploy-check.sh",
        "deploy": "bash deploy-lambda.sh",
        "migrate": "node simple-migrate.js",
        "lambda:dev": "dotenv -e .env -- serverless offline",
        "lambda:deploy": "NODE_OPTIONS='--no-deprecation' serverless deploy",
        "lambda:deploy:prod": "NODE_OPTIONS='--no-deprecation' serverless deploy --stage prod",
        "lambda:info": "serverless info",
        "lambda:logs": "serverless logs -f",
        "lambda:remove": "serverless remove",
        "preview:package": "bash preview-lambda-package.sh"
    },
    "type": "module",
    "dependencies": {
        "@anthropic-ai/claude-code": "^2.0.17",
        "@aws-sdk/client-ses": "^3.906.0",
        "bcryptjs": "^2.4.3",
        "dotenv": "^16.3.1",
        "form-data": "^4.0.4",
        "jsonwebtoken": "^9.0.2",
        "mailgun.js": "^12.1.1",
        "pg": "^8.11.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "devDependencies": {
        "@types/pg": "^8.10.9",
        "dotenv-cli": "^7.3.0",
        "express": "^4.18.2",
        "http-proxy-middleware": "^2.0.6",
        "serverless": "^4.21.1",
        "serverless-esbuild": "^1.55.1",
        "serverless-offline": "^14.4.0"
    }
}
